  <data name="GetToken" xml:space="preserve">
    <value>// ===================== Fetch 拦截 =====================
const originalFetch = window.fetch || unsafeWindow.fetch;
//window.fetch= 有时不启作用，使用unsafeWindow.fetch=
Window.fetch = function (input, init) {
    const url = typeof input === "string" ? input : input.url;
    console.log("🚀 🚀 🚀 [Fetch] 请求:", url);
    // 拦截指定 token URL   url.includes("https://login.microsoftonline.com/common/oauth2/v2.0/token"   //if (/oauth|token/.test(url))
    if (url.includes("/token")) {
        console.log("🚀 拦截 Fetch 请求:", url);

        return originalFetch(input, init).then(response =&gt; {
            console.log("📦 响应状态:", response.status);

            //return respCloneForJson.json()
            response.clone().json().then(data =&gt; {
                console.log("📄 响应数据:", data);
                if (data.token_type &amp;&amp; data.access_token) {
                    console.log("🚀 🚀 🚀[Fetch] Authorization:", `${data.token_type} ${data.access_token}`);
                    GM_setClipboard(data.refresh_token);
                    //window.localStorage.setItem("token", JSON.stringify(data));
                    //debugger
                }
            }).catch(err =&gt; console.warn("[Fetch] JSON 非法", err));

            response.clone().text().then(text =&gt; {
                localStorage.setItem("token", text);
                console.log("📄 token 原始响应文本已存储");
            }).catch(err =&gt; console.warn("[Fetch] text 读取失败", err));

            return response; // 返回原始响应，保持 fetch 行为不变
        });
    }

    // 对其他请求不拦截，直接执行
    return originalFetch(input, init);
};</value>
